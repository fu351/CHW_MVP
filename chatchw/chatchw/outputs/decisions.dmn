        <dmn:outputEntry id="danger_sign_prolonged_fever">
          <dmn:text>false</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="clinic_referral_prolonged_fever">
          <dmn:text>true</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="reason_prolonged_fever">
          <dmn:text>"prolonged_fever"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="ref_prolonged_fever">
          <dmn:text>"pXX"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="advice_prolonged_fever">
          <dmn:text>"[]"</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
    </dmn:decisionTable>
