      <dmn:output id="clinic_referral_output" name="clinic_referral" typeRef="boolean"/>
      <dmn:output id="reason_output" name="reason" typeRef="string"/>
      <dmn:output id="ref_output" name="ref" typeRef="string"/>
      <dmn:output id="advice_output" name="advice" typeRef="string"/>
      <dmn:rule>
        <dmn:inputEntry id="malaria_area_entry">
          <dmn:text>true</dmn:text>
        </dmn:inputEntry>
        <dmn:inputEntry id="fever_entry">
          <dmn:text>true</dmn:text>
        </dmn:inputEntry>
        <dmn:inputEntry id="fever_duration_days_entry">
          <dmn:text>[3..6]</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry id="triage_malaria_area">
          <dmn:text>"clinic"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="danger_sign_malaria_area">
          <dmn:text>false</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="clinic_referral_malaria_area">
          <dmn:text>true</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="reason_malaria_area">
          <dmn:text>"fever_in_malaria_area"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="ref_malaria_area">
          <dmn:text>"pXX"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="advice_malaria_area">
          <dmn:text>"[]"</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
      <dmn:rule>
        <dmn:inputEntry id="fever_high_temp_entry">
          <dmn:text>true</dmn:text>
        </dmn:inputEntry>
        <dmn:inputEntry id="temperature_c_entry">
          <dmn:text>&gt;= 39</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry id="triage_high_temp">
          <dmn:text>"clinic"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="danger_sign_high_temp">
          <dmn:text>false</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="clinic_referral_high_temp">
          <dmn:text>true</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="reason_high_temp">
          <dmn:text>"high_fever"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="ref_high_temp">
          <dmn:text>"pXX"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="advice_high_temp">
          <dmn:text>"[&quot;tepid sponging if safe&quot;]"</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
      <dmn:rule>
        <dmn:inputEntry id="fever_prolonged_entry">
          <dmn:text>true</dmn:text>
        </dmn:inputEntry>
        <dmn:inputEntry id="fever_duration_days_prolonged_entry">
