  <dmn:decision id="decide_nutrition" name="decide_nutrition">
    <dmn:decisionTable hitPolicy="FIRST">
      <dmn:input id="muac_mm_input" label="muac_mm">
        <dmn:inputExpression typeRef="number">
          <dmn:text>muac_mm</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:output id="triage_output" name="triage" typeRef="string"/>
      <dmn:output id="danger_sign_output" name="danger_sign" typeRef="boolean"/>
      <dmn:output id="clinic_referral_output" name="clinic_referral" typeRef="boolean"/>
      <dmn:output id="reason_output" name="reason" typeRef="string"/>
      <dmn:output id="ref_output" name="ref" typeRef="string"/>
      <dmn:output id="advice_output" name="advice" typeRef="string"/>
      <dmn:rule>
        <dmn:inputEntry id="muac_mm_yellow_entry">
          <dmn:text>[115..124]</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry id="triage_yellow">
          <dmn:text>"clinic"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="danger_sign_yellow">
          <dmn:text>false</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="clinic_referral_yellow">
          <dmn:text>true</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="reason_yellow">
          <dmn:text>"moderate_acute_malnutrition"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="ref_yellow">
          <dmn:text>"pXX"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="advice_yellow">
          <dmn:text>"[]"</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
      <dmn:rule>
        <dmn:inputEntry id="muac_mm_green_entry">
          <dmn:text>&gt;= 125</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry id="triage_green">
          <dmn:text>"home"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="danger_sign_green">
          <dmn:text>false</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="clinic_referral_green">
          <dmn:text>false</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="reason_green">
          <dmn:text>"normal_nutrition"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="ref_green">
          <dmn:text>"pXX"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="advice_green">
          <dmn:text>"[&quot;continue feeding&quot;]"</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
    </dmn:decisionTable>
  </dmn:decision>

  <dmn:decision id="aggregate_final" name="aggregate_final">
    <dmn:decisionTable hitPolicy="FIRST">
      <dmn:input id="danger_sign_input" label="danger_sign">
        <dmn:inputExpression typeRef="boolean">
          <dmn:text>decide_danger_signs.danger_sign</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="clinic_referral_diarrhea_input" label="clinic_referral_diarrhea">
        <dmn:inputExpression typeRef="boolean">
          <dmn:text>decide_diarrhea.clinic_referral</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="clinic_referral_fever_malaria_input" label="clinic_referral_fever_malaria">
        <dmn:inputExpression typeRef="boolean">
          <dmn:text>decide_fever_malaria.clinic_referral</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="clinic_referral_respiratory_input" label="clinic_referral_respiratory">
        <dmn:inputExpression typeRef="boolean">
          <dmn:text>decide_respiratory.clinic_referral</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="clinic_referral_nutrition_input" label="clinic_referral_nutrition">
        <dmn:inputExpression typeRef="boolean">
          <dmn:text>decide_nutrition.clinic_referral</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:output id="triage_output" name="triage" typeRef="string"/>
      <dmn:output id="reason_output" name="reason" typeRef="string"/>
      <dmn:output id="ref_output" name="ref" typeRef="string"/>
      <dmn:output id="advice_output" name="advice" typeRef="string"/>
      <dmn:rule>
        <dmn:inputEntry id="danger_sign_entry">
          <dmn:text>true</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry id="triage_danger_sign">
          <dmn:text>"hospital"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="reason_danger_sign">
          <dmn:text>"danger_sign"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="ref_danger_sign">
          <dmn:text>"aggregator"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="advice_danger_sign">
          <dmn:text>"[]"</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
      <dmn:rule>
        <dmn:inputEntry id="clinic_referral_any_entry">
          <dmn:text>true</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry id="triage_clinic_referral">
          <dmn:text>"clinic"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="reason_clinic_referral">
          <dmn:text>"needs_clinic_referral"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="ref_clinic_referral">
          <dmn:text>"aggregator"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="advice_clinic_referral">
          <dmn:text>"[]"</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
      <dmn:rule>
        <dmn:inputEntry id="home_care_entry">
          <dmn:text>false</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry id="triage_home_care">
          <dmn:text>"home"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="reason_home_care">
          <dmn:text>"home_care"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="ref_home_care">
          <dmn:text>"aggregator"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="advice_home_care">
          <dmn:text>"[&quot;fluids, feeding, return if worse&quot;]"</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
    </dmn:decisionTable>
    <dmn:informationRequirement>
      <dmn:requiredDecision href="#decide_danger_signs"/>
    </dmn:informationRequirement>
    <dmn:informationRequirement>
      <dmn:requiredDecision href="#decide_diarrhea"/>
    </dmn:informationRequirement>
    <dmn:informationRequirement>
      <dmn:requiredDecision href="#decide_fever_malaria"/>
    </dmn:informationRequirement>
    <dmn:informationRequirement>
      <dmn:requiredDecision href="#decide_respiratory"/>
    </dmn:informationRequirement>
    <dmn:informationRequirement>
      <dmn:requiredDecision href="#decide_nutrition"/>
    </dmn:informationRequirement>
  </dmn:decision>
</dmn:definitions>