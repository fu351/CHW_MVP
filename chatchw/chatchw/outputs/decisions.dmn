      <dmn:output id="advice_output" name="advice" typeRef="string"/>
      <dmn:rule>
        <dmn:inputEntry id="blood_in_stool_entry">
          <dmn:text>true</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry id="triage_blood_in_stool">
          <dmn:text>"clinic"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="danger_sign_blood_in_stool">
          <dmn:text>false</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="clinic_referral_blood_in_stool">
          <dmn:text>true</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="reason_blood_in_stool">
          <dmn:text>"dysentery"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="ref_blood_in_stool">
          <dmn:text>"pXX"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="advice_blood_in_stool">
          <dmn:text>"[&quot;start ORS if available&quot;]"</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
      <dmn:rule>
        <dmn:inputEntry id="diarrhea_duration_days_entry">
          <dmn:text>&gt;= 14</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry id="triage_diarrhea_duration_days">
          <dmn:text>"clinic"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="danger_sign_diarrhea_duration_days">
