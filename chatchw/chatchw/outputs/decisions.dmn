        </dmn:outputEntry>
        <dmn:outputEntry id="reason_diarrhea_duration_days">
          <dmn:text>"persistent_diarrhea"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="ref_diarrhea_duration_days">
          <dmn:text>"pXX"</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry id="advice_diarrhea_duration_days">
          <dmn:text>"[&quot;continue ORS, monitor&quot;]"</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
    </dmn:decisionTable>
  </dmn:decision>

  <dmn:decision id="decide_fever_malaria" name="decide_fever_malaria">
    <dmn:decisionTable hitPolicy="FIRST">
      <dmn:input id="malaria_area_input" label="malaria_area">
